        public void Main()
        {
            // READ variables
            string name = Dts.Variables["User::UserName"].Value.ToString();
            int counter = (int)Dts.Variables["User::Counter"].Value;

            // Modify the counter
            counter = counter + 1;

            // WRITE back to the variable
            Dts.Variables["User::Counter"].Value = counter;

